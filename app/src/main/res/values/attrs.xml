<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ToastTextView">
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textGravity">
            <flag name="left" value="1" />
            <flag name="center" value="2" />
            <flag name="right" value="3" />
        </attr>
        <attr name="backGround" format="reference|color" />
        <attr name="iconType">
            <flag name="none" value="1" />
            <flag name="close" value="2" />
            <flag name="arrow" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="layoutDirection" format="enum">
            <enum name="ltr" value="0" />
            <enum name="rtl" value="1" />
        </attr>
        <attr name="debugDraw" format="boolean" />
        <attr name="weightDefault" format="float" />
        <attr name="maxLines" format="integer" />
        <attr name="spacingLength" format="dimension" />
        <attr name="spacingThickness" format="dimension" />
        <attr name="android:gravity" />
    </declare-styleable>

    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_newLine" format="boolean" />
        <attr name="layout_weight" format="float" />
        <attr name="android:layout_gravity" />
    </declare-styleable>

    <declare-styleable name="FixedImageView">
        <attr name="whRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="SectorBar">
        <attr name="backgroundColor" format="color" />
        <attr name="percentColor" format="color" />
        <attr name="percent" format="float" />
        <attr name="drawStyle">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
        <attr name="strokeWidth" format="integer" />
    </declare-styleable>

    <declare-styleable name="SlidingButton">
        <attr name="frame" format="reference"/>
        <attr name="sliderOn" format="reference"/>
        <attr name="sliderOff" format="reference"/>
        <attr name="mask" format="reference"/>
        <attr name="bar" format="reference"/>
        <attr name="barOff" format="reference"/>
        <attr name="barOn" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AlertDialog">
        <attr name="android:layout" format="reference" />
        <attr name="listLayout" format="reference" />
        <attr name="multiChoiceItemLayout" format="reference" />
        <attr name="singleChoiceItemLayout" format="reference" />
        <attr name="listItemLayout" format="reference" />
        <attr name="progressLayout" format="reference" />
        <attr name="horizontalProgressLayout" format="reference" />
    </declare-styleable>
</resources>